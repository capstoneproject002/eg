%tr
  %td= user.name
  %td= user.email
  %td= l user.created_at
  %td.action
    = link_to([:hq, user], class: 'btn btn-success', toogle: 'tooltip', title: t('tooltips.zoom')) do
      %i.icon-zoom-in
    = link_to(edit_hq_user_path(user), class: 'btn btn-success', toogle: 'tooltip', title: t('tooltips.zoom')) do
      %i.icon-pencil